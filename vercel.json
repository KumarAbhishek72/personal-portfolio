{
  "cleanUrls": true,
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/dist/output.css",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000" }]
    },
    {
      "source": "/assets/pics/**",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000" }]
    }
  ]
}
